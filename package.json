{
  "name": "hundred-to-one",
  "version": "1.0.0",
  "description": "Табло и панель управления для игры \"100 к 1\"",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ hundred-to-one --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "build-win": "electron-packager ./ hundred-to-one --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds"
  },
  "repository": "https://github.com/kot13/hundred-to-one",
  "author": "kot_tdf@mail.ru",
  "devDependencies": {
    "electron": "~1.8.1",
    "electron-packager": "^9.1.0"
  },
  "dependencies": {
    "electron-json-storage": "^3.1.1",
    "handlebars": "^4.0.10"
  }
}
